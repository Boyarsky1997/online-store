<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.github.boyarsky1997.store.servlets.LoginServlet</servlet-class>
    </servlet>
    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>